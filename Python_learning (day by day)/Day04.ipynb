    "## Loop: Factorial Calculator"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a2a058ea",
   "metadata": {},
   "outputs": [],
   "source": [
    "num= int(input(\"Enter a number: \"))\n",
    "factorial  = 1\n",
    "\n",
    "for i in range (1, num + 1):\n",
    "    factorial *= i\n",
    "\n",
    "print(\"Factorial of \",num, \"is\", factorial)"
   ]
